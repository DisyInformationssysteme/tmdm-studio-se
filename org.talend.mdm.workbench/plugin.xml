<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="Talend MDM Workbench"
            id="org.talend.mdm.workbench">
      </category>
      <view
            allowMultiple="false"
            category="org.talend.mdm.workbench"
            class="com.amalto.workbench.views.ServerView"
            icon="icons/talend-picto-small.gif"
            id="org.talend.mdm.workbench.views.ServerView"
            name="MDM Server View">
            <!--name="Talend MDM Server View"-->
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.amalto.workbench.editors.XObjectEditor"
            default="false"
            icon="icons/talend-picto-small.gif"
            id="com.amalto.workbench.editors.XObjectEditor"
            name="com.amalto.workbench.editors.XObjectEditor"/>
      <editor
            class="com.amalto.workbench.editors.XObjectBrowser"
            default="false"
            icon="icons/talend-picto-small.gif"
            id="com.amalto.workbench.editors.XObjectBrowser"
            name="com.amalto.workbench.editors.XObjectBrowser"/>
      <editor
            class="com.amalto.workbench.editors.XObjectRevisionBrowser"
            default="false"
            icon="icons/talend-picto-small.gif"
            id="com.amalto.workbench.editors.XObjectRevisionBrowser"
            name="com.amalto.workbench.editors.XObjectRevisionBrowser">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="com.amalto.workbench.views.MDMPerspective"
            fixed="true"
            icon="icons/talend-picto-small.gif"
            id="org.talend.mdm.perspective"
            name="Master Data Management">
      </perspective>
   </extension>
   <!-- add the following to let MDM perspective to be show in TIS/TDQ -->
  <extension
         point="org.talend.core.talendperspectives">
      <TalendPerspective
            refPerspectiveId="org.talend.mdm.perspective"
            talendPerspectiveId="org.talend.mdm.perspective">
      </TalendPerspective>
   </extension>
  <extension
        point="org.eclipse.ui.intro">
     <intro
           class="org.eclipse.ui.intro.config.CustomizableIntroPart"
           id="org.talend.mdm.workbench.intro">
     </intro>
     <introProductBinding
           introId="org.talend.mdm.workbench.intro"
           productId="org.talend.mdm.workbench.product">
     </introProductBinding>
  </extension>
  <extension
        point="org.eclipse.ui.intro.config">
     <config
           content="introContent.xml"
           id="org.talend.mdm.workbench.introConfigId"
           introId="org.talend.mdm.workbench.intro">
        <presentation
              home-page-id="root">
           <implementation
                 kind="html"
                 os="win32,linux,macosx"
                 style="content/shared.css">
           </implementation>
        </presentation>
     </config>
  </extension>
  <extension
        id="product"
        point="org.eclipse.core.runtime.products">
     <product
           application="org.eclipse.ui.ide.workbench"
           name="Master Data Management">
        <property
              name="appName"
              value="Master Data Management">
        </property>
        <property
              name="aboutImage"
              value="icons/talend-picto-small.gif">
        </property>
        <property
              name="aboutText"
              value="Master Data Management">
        </property>
        <property
              name="windowImages"
              value="icons/appli_16x16.gif,icons/appli_32x32.gif,icons/appli_48x48.gif">
        </property>
        <property
              name="startupForegroundColor"
              value="FFFFFF">
        </property>
        <property
              name="startupMessageRect"
              value="7,252,445,20">
        </property>
        <property
              name="startupProgressRect"
              value="5,275,445,15">
        </property>
        <property
              name="preferenceCustomization"
              value="plugin_customization.ini">
        </property>
     </product>
  </extension>


   
</plugin>
