<?xml version="1.0" encoding="UTF-8"?>
<dwr>
    <allow>
        <create creator="new" javascript="ItemsBrowserInterface">
            <param name="class" value="com.amalto.webapp.v3.itemsbrowser.dwr.ItemsBrowserDWR"/>
        </create>
        
        <convert converter="bean" match="com.amalto.webapp.v3.itemsbrowser.bean.TreeNode"/>
        <convert converter="bean" match="com.amalto.webapp.v3.itemsbrowser.bean.View"/>
        <convert converter="bean" match="com.amalto.webapp.v3.itemsbrowser.bean.Restriction"/>
        
        <convert converter="bean" match="java.lang.NullPointerException"/>
        <convert converter="bean" match="java.lang.Exception"/>
        <convert converter="bean" match="java.lang.StackTraceElement"/>
        
    </allow>

</dwr>
